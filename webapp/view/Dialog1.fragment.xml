<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form">
    <Dialog id="id1Dialog" afterClose="dialogAftercloseadd">
        <f:SimpleForm layout="ResponsiveGridLayout" id="editmode1" editable="false">
            <f:content>
              
                <Label text="Name" />
                <Input id="name" />
                <Label text="Zemprole" />
                <Input id="Zemprole" />
                <Label text="Zgender" />
                <Input id="Zgender" />
                <Label text="Znumber" />
                <Input id="Znumber" />
                 <Label text="Zcity" />
                <Input id="Zcity" />
               
            </f:content>
        </f:SimpleForm>
        <HBox justifyContent="End" alignItems="End">
            <Button id="ok" type="Transparent" text="Add" press="AddPress" />
            <Button id="ok1" type="Transparent" text="Cancel" press="CancelPress1" />
            <Button id="ok2" type="Transparent" text="home" press="homePress1" />
        </HBox>
    </Dialog>
</core:FragmentDefinition>
